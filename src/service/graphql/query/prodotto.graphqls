query prodotto ($id:Int){
  prodotto(id:$id){
        id,
        codice,
        descrizione,
        tipo,
        um,
        trattamento,
        tempoLavorazione,
        foto
        componenti{
            id
            um,
            quantita,
            prodottoDaLavorare{
                id,
                codice,
                descrizione,
                um,
                tipo
            }
        }
  }
}